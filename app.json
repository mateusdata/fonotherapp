{
  "expo": {
    "name": "FonotherApp",
    "slug": "fonotherapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#159797"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.projetofono.fonotherapp",
      "infoPlist": {
        "NSFileProtection": "CompleteUnlessOpen",
        "NSFaceIDUsageDescription": "Usamos o Face ID para facilitar o login e proteger seus dados.",
        "NSCameraUsageDescription": "Este aplicativo usa a c√¢mera para permitir tirar foto de perfil.",
        "ITSAppUsesNonExemptEncryption": false,
        "LSApplicationQueriesSchemes": [
          "whatsapp"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.projetofono.fonotherapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "plugins": [
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": false,
          "supportsPictureInPicture": false
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos to choose a profile picture for the user.",
          "cameraPermission": "Allow $(PRODUCT_NAME) to access the camera to take a profile picture for the user"
        }
      ],
      "expo-local-authentication"
    ],
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@expo-google-fonts/poppins",
          "tamagui"
        ]
      }
    },
    "extra": {
      "eas": {
        "projectId": "8cf0d8e1-f5d4-443c-9b8b-9045da5263d4"
      }
    }
  }
}